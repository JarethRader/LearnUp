{
  "name": "whiteboard",
  "version": "0.1.0",
  "description": "Whiteboard API for LearnUp Phonetics learning board",
  "main": "src/server.ts",
  "scripts": {
    "start": "env NODE_ENV=development nodemon --watch src src/server.ts",
    "docker": "env NODE_ENV=docker nodemon --watch src src/server.ts",
    "build": "tsc -p tsconfig-build.json",
    "test": "env NODE_ENV=test jest --config ./jest.config.json --detectOpenHandles --runInBand",
    "deploy:dev": "sh deploy.dev.sh",
    "deploy:prod": "sh deploy.prod.sh"
  },
  "author": "Jareth Rader <jarethrdev@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/morgan": "^1.9.2",
    "@types/pg": "^7.14.11",
    "@types/sequelize": "^4.28.9",
    "eslint": "^7.19.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.5"
  },
  "dependencies": {
    "bson-objectid": "^1.3.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.15",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
