    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.25",
    "@types/uuid": "^8.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.19.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
